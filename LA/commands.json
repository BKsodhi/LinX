{
  "check linux version": "uname -a",
  "show linux distribution": "lsb_release -a",
  "list ubuntu flavors": "echo 'Ubuntu, Kubuntu, Xubuntu, Lubuntu, Ubuntu Server'",
  "open gparted partition tool": "sudo gparted",
  "install software": "sudo apt install <package>",
  "update system": "sudo apt update && sudo apt upgrade -y",
  "list files": "ls -l",
  "change directory": "cd <directory>",
  "show current directory": "pwd",
  "copy file": "cp <source> <destination>",
  "move file": "mv <source> <destination>",
  "remove file": "rm <filename>",
  "make directory": "mkdir <dirname>",
  "remove directory": "rmdir <dirname>",
  "view file content": "cat <filename>",
  "edit file with nano": "nano <filename>",
  "edit file with vim": "vim <filename>",
  "show top processes": "top",
  "kill process": "kill <pid>",
  "check running processes": "ps aux",
  "check file permissions": "ls -l",
  "change permissions": "chmod 755 <filename>",
  "change ownership": "chown <user>:<group> <filename>",
  "install package": "sudo apt install <package>",
  "remove package": "sudo apt remove <package>",
  "install deb file": "sudo dpkg -i <package>.deb",
  "check ip address": "ip a",
  "ping google": "ping -c 4 google.com",
  "show routing table": "netstat -rn",
  "traceroute a domain": "traceroute google.com",
  "configure network with netplan": "sudo netplan apply",
  "configure network with nmcli": "nmcli device show",
  "add user": "sudo adduser <username>",
  "delete user": "sudo deluser <username>",
  "add group": "sudo groupadd <groupname>",
  "modify user": "sudo usermod -aG <group> <username>",
  "view passwd file": "cat /etc/passwd",
  "view group file": "cat /etc/group",
  "create shell script": "nano script.sh",
  "run shell script": "bash script.sh",
  "list disk usage": "df -h",
  "check directory size": "du -sh <directory>",
  "partition disk": "sudo fdisk /dev/sdX",
  "mount disk": "sudo mount /dev/sdX1 /mnt",
  "unmount disk": "sudo umount /mnt",
  "create tar backup": "tar -cvf backup.tar <directory>",
  "restore tar backup": "tar -xvf backup.tar",
  "backup with rsync": "rsync -av <source> <destination>",
  "check disk with fsck": "sudo fsck /dev/sdX1",
  "view kernel messages": "dmesg | less",
  "start firewall": "sudo ufw enable",
  "check firewall status": "sudo ufw status",
  "allow port in firewall": "sudo ufw allow 22",
  "view system logs": "journalctl -xe",
  "monitor with htop": "htop",
  "performance tuning": "sudo sysctl -a | less",
  "set kernel parameter": "sudo sysctl -w net.ipv4.ip_forward=1",
  "configure cron job": "crontab -e",
  "view cron jobs": "crontab -l",
  "create at job": "at now + 1 minute",
  "list at jobs": "atq",
  "check service status": "systemctl status <service>",
  "start service": "sudo systemctl start <service>",
  "enable service": "sudo systemctl enable <service>",
  "use ssh": "ssh <user>@<ip-address>",
  "block ssh brute force": "sudo fail2ban-client status sshd",
  "check virtualization support": "egrep -c '(vmx|svm)' /proc/cpuinfo",
  "launch virtual machine": "virt-manager",
  "start docker container": "docker start <container>",
  "list docker containers": "docker ps -a",
  "run lxd container": "lxc launch ubuntu:22.04 mycontainer",
  "install microk8s": "sudo snap install microk8s --classic",
  "check apparmor status": "sudo aa-status"
}
